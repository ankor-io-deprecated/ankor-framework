<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<Tab fx:id="tab" closable="true" xmlns:fx="http://javafx.com/fxml" fx:controller="at.irian.ankorman.sample2.fxclient.task.TasksController">
<AnchorPane id="body" minWidth="650.0" prefHeight="650.0" prefWidth="650.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <VBox alignment="TOP_CENTER" prefWidth="550.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <VBox alignment="CENTER" maxWidth="550.0" VBox.margin="$x1" VBox.vgrow="ALWAYS">
          <children>
            <Label id="header" alignment="CENTER" text="todos" textAlignment="CENTER">
              <font>
                <Font size="70.0" />
              </font>
              <VBox.margin>
                <Insets bottom="10.0" top="10.0" />
              </VBox.margin>
            </Label>
            <GridPane VBox.vgrow="ALWAYS">
              <children>
                <Pane id="bigShadow" style="-fx-background-color: #fff;" styleClass="bigShadow" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                <AnchorPane id="footer" prefHeight="30.0" styleClass="boxShadow, paper" GridPane.columnIndex="0" GridPane.rowIndex="2">
                  <children>
                    <HBox id="filters" alignment="CENTER" prefHeight="35.0" prefWidth="530.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                      <children>
                        <Button mnemonicParsing="false" text="All" />
                        <Button mnemonicParsing="false" text="Active" />
                        <Button mnemonicParsing="false" text="Completed" />
                      </children>
                      <padding>
                        <Insets top="2.0" />
                      </padding>
                    </HBox>
                    <HBox id="todoCount" alignment="CENTER_LEFT" prefHeight="35.0" spacing="5.0" AnchorPane.leftAnchor="10.0">
                      <children>
                        <Label fx:id="todoCountNum" style="-fx-font-weight: bold;" text="3" />
                        <Label text="items left" />
                      </children>
                    </HBox>
                    <Button fx:id="clearCompleted" alignment="CENTER_RIGHT" contentDisplay="CENTER" layoutY="7.0" mnemonicParsing="false" prefHeight="20.0" text="Clear completed (1)" textAlignment="CENTER" AnchorPane.rightAnchor="10.0">
                      <effect>
                        <InnerShadow blurType="TWO_PASS_BOX" color="#00000033" height="1.0" radius="0.0" width="0.0" />
                      </effect>
                    </Button>
                  </children>
                  <GridPane.margin>
                    <Insets left="10.0" right="10.0" />
                  </GridPane.margin>
                </AnchorPane>
                <Pane styleClass="boxShadow, paper" GridPane.columnIndex="0" GridPane.rowIndex="1">
                  <GridPane.margin>
                    <Insets left="5.0" right="5.0" />
                  </GridPane.margin>
                </Pane>
                <AnchorPane id="" GridPane.columnIndex="0" GridPane.rowIndex="0">
                  <children>
                    <VBox id="todoApp" styleClass="boxShadow" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                      <children>
                        <Pane id="pane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="15.0" />
                        <HBox alignment="CENTER_LEFT" maxHeight="65.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="65.0" style="-fx-background-color: rgba(0, 0, 0, 0.02);" styleClass="task">
                          <children>
                            <CheckBox alignment="CENTER" mnemonicParsing="false" prefWidth="45.0" />
                            <TextField fx:id="newTodo" onAction="#newTodo" promptText="What needs to be done?" HBox.hgrow="ALWAYS">
                              <HBox.margin>
                                <Insets left="10.0" right="10.0" top="3.0" />
                              </HBox.margin>
                            </TextField>
                          </children>
                        </HBox>
                        <ScrollPane id="ScrollPane" fitToHeight="true" fitToWidth="true" prefViewportHeight="130.5" prefViewportWidth="550.0" style="-fx-background-color:transparent;" VBox.vgrow="ALWAYS">
                          <content>
                            <VBox fx:id="tasks" />
                          </content>
                        </ScrollPane>
                      </children>
                    </VBox>
                    <Pane id="lines" layoutX="40.0" prefHeight="208.0" prefWidth="4.0" AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="16.0" />
                  </children>
                </AnchorPane>
              </children>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="3.0" minHeight="3.0" prefHeight="3.0" vgrow="NEVER" />
                <RowConstraints maxHeight="35.0" minHeight="35.0" prefHeight="35.0" vgrow="NEVER" />
              </rowConstraints>
              <VBox.margin>
                <Insets fx:id="x1" />
              </VBox.margin>
            </GridPane>
            <VBox id="info" prefHeight="147.0" prefWidth="600.0" spacing="12.0">
              <children>
                <HBox alignment="TOP_CENTER">
                  <children>
                    <Label prefHeight="19.0" styleClass="footerText" text="Double-click to edit a todo" HBox.hgrow="ALWAYS" />
                  </children>
                </HBox>
                <HBox alignment="TOP_CENTER">
                  <children>
                    <Label prefHeight="19.0" styleClass="footerText" text="Written by" HBox.hgrow="ALWAYS" />
                    <Hyperlink styleClass="footerText" text="Florian Klampfer" HBox.hgrow="ALWAYS" />
                  </children>
                </HBox>
                <HBox alignment="TOP_CENTER">
                  <children>
                    <Label prefHeight="19.0" styleClass="footerText" text="Part of" HBox.hgrow="ALWAYS" />
                    <Hyperlink styleClass="footerText" text="TodoMVC" HBox.hgrow="ALWAYS" />
                  </children>
                </HBox>
              </children>
              <padding>
                <Insets top="25.0" />
              </padding>
            </VBox>
          </children>
        </VBox>
      </children>
      <padding>
        <Insets top="25.0" />
      </padding>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@style.css" />
  </stylesheets>
</AnchorPane>
</Tab>
